# ===============================================
# ORCHESTRIX FRONTEND - PRODUCTION CONFIGURATION
# ===============================================

# API Configuration
REACT_APP_API_URL=https://api.orchestrix.com/api
REACT_APP_WS_URL=wss://api.orchestrix.com/ws
REACT_APP_API_TIMEOUT=10000

# Application Information
REACT_APP_VERSION=1.0.0
REACT_APP_NAME=Orchestrix
REACT_APP_ENVIRONMENT=production
REACT_APP_BUILD_NUMBER=${BUILD_NUMBER:prod-build}

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=true
BUILD_PATH=build

# Feature Flags (Production)
REACT_APP_ENABLE_DEBUG=false
REACT_APP_ENABLE_MOCK_DATA=false
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true

# Performance & Security
REACT_APP_LOG_LEVEL=error
REACT_APP_ENABLE_REDUX_DEVTOOLS=false

# Security (Production - Strict)
REACT_APP_SESSION_TIMEOUT=900000
REACT_APP_IDLE_TIMEOUT=300000
REACT_APP_MAX_LOGIN_ATTEMPTS=3

# UI Configuration
REACT_APP_DEFAULT_THEME=light
REACT_APP_ENABLE_THEME_SWITCHER=false
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_PAGINATION_SIZE=15

# External Services (Production)
REACT_APP_ANALYTICS_ID=UA-PRODUCTION-ORCHESTRIX
REACT_APP_ERROR_REPORTING_DSN=https://errors.orchestrix.com/dsn

# Production URLs
REACT_APP_DOCS_URL=https://docs.orchestrix.com
REACT_APP_SUPPORT_URL=https://support.orchestrix.com

# CDN Configuration
PUBLIC_URL=https://cdn.orchestrix.com
REACT_APP_CDN_URL=https://assets.orchestrix.com